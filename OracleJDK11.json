    "extract_dir": "jdk-11.0.3",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://www.oracle.com/technetwork/java/javase/downloads/jdk11-downloads-5066655.html",
        "re": "otn/java/jdk/(?<path>(?<major>[\\d.]+)\\+(?<build>[\\d]+)/(?<hash>[a-fA-F0-9]{32})/jdk-(?:.*?)_windows-x64_bin.zip)",
        "replace": "${major}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.oracle.com/otn/java/jdk/$matchPath"
            }
        },
        "hash": {
            "url": "https://www.oracle.com/technetwork/java/javase/downloads/jdk11-downloads-5066655.html",
            "find": "$basename.*([a-fA-F0-9]{64})\"};"
        },
        "extract_dir": "jdk-$matchMajor"
    }
}
