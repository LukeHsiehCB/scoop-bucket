{
    "homepage": "https://aimp.ru",
    "version": "4.60.2153",
    "description": "A music player and library manager.",
    "license": "LGPL-2.1-only",
    "url": "http://aimp.su/storage/3d1c6f8e174ea83ff14be17050fe4386/aimp_4.60.2153_no-setup.zip",
    "hash": "217815a3cb47a1ddaafc02c91b267eb2d59591770e6952d17c7ea421ea3a90fa",
    "persist": [
        "Profile",
        "!Backup"
    ],
    "bin": "aimp.exe",
    "shortcuts": [
        [
            "aimp.exe",
            "AIMP"
        ],
        [
            "AIMPate.exe",
            "AIMP Advanced Tag Editer"
        ],
        [
            "AIMPac.exe",
            "AIMP Audio Converter"
        ]
    ],
    "checkver": {
        "url": "http://www.aimp.ru/?do=download&os=windows",
        "regex": "<h1>AIMP v(?<ver>[\\d.]+), build (?<build>\\d+) \\(No Installer\\)</h1>[\\w\\W]+?<nobr>SHA256:[\\w\\W]+?(?<hash>[a-fA-F0-9]{64})</nobr>",
        "replace": "${ver}.${build}"
    },
    "autoupdate": {
        "url": "http://aimp.su/storage/$matchHash/aimp_$version.exe",
        "hash": {
            "url": "https://www.aimp.ru/?do=download&os=windows",
            "find": "No Installer.*?SHA256: $sha256"
        }
    }
}
