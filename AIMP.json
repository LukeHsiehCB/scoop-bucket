{
    "homepage": "http://aimp.ru",
    "version": "4.60.2144",
    "description": "AIMP is a music player and library manager",
    "license": "LGPL-2.1-only",
    "url": "http://aimp.su/storage/e38e6a39351e1409b5075c89baa3ff32/aimp_4.60.2144.exe",
    "hash": "c59179e832fc850046042bf0d994ec6e324fb62e3128904b959aedd2d78b09f1",
    "installer": {
        "args": [
            "/AUTO=$dir",
            "/PORTABLE",
            "/SILENT"
        ],
        "keep": false
    },
    "persist": [
        "Profile",
        "!Backup"
    ],
    "bin": "aimp.exe",
    "shortcuts": [
        [
            "aimp.exe",
            "AIMP"
        ],
        [
            "AIMPate.exe",
            "AIMP Advanced Tag Editer"
        ],
        [
            "AIMPac.exe",
            "AIMP Audio Converter"
        ]
    ],
    "checkver": {
        "url": "http://www.aimp.ru/?do=download&os=windows",
        "regex": "http://aimp.su/storage/(?<hash>[a-fA-F0-9]{32})/aimp_(?<version>[\\d.]+).exe"
    },
    "autoupdate": {
        "url": "http://aimp.su/storage/$matchHash/aimp_$version.exe",
        "hash": {
            "url": "http://www.aimp.ru/?do=download&os=windows",
            "find": "SHA256: $sha256"
        }
    }
}
